<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rocketmq.demo.repository.AccountInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.rocketmq.demo.model.dataobject.AccountInfoDO">
        <id column="id" property="id" />
        <result column="account_name" property="accountName" />
        <result column="account_no" property="accountNo" />
        <result column="account_password" property="accountPassword" />
        <result column="account_balance" property="accountBalance" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_name, account_no, account_password, account_balance
    </sql>

</mapper>
